<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.zzmdemo.mapper.UserMapper">

    <select id="userLogin" resultType="com.example.zzmdemo.entity.SysUser">
      select * from sys_user where login_name=#{loginName} and password=#{password}
    </select>
    <select id="userTest" resultType="com.example.zzmdemo.entity.SysUser">
      select * from sys_user
    </select>
</mapper>